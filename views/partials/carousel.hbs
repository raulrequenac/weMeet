<div class="d-flex justify-content-center">
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carouselExampleIndicators">
    <ol class="carousel-indicators">
      {{#each element.images}}
        <li data-target="#carouselExampleIndicators" data-slide-to="{{@index}}" class="active"></li>
      {{/each}}
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block carousel-img" src="{{element.images.[0]}}" alt="First slide">
      </div>
      {{#if element.images.[1]}}
        <div class="carousel-item">
          <img class="d-block carousel-img" src="{{element.images.[1]}}" alt="Second slide">
        </div>
      {{/if}}
      {{#if element.images.[2]}}
        <div class="carousel-item">
          <img class="d-block carousel-img" src="{{element.images.[2]}}" alt="Third slide">
        </div>
      {{/if}}
      {{#if element.images.[3]}}
        <div class="carousel-item">
          <img class="d-block carousel-img" src="{{element.images.[3]}}" alt="Third slide">
        </div>
      {{/if}}
      {{#if element.images.[4]}}
        <div class="carousel-item">
          <img class="d-block carousel-img" src="{{element.images.[4]}}" alt="Third slide">
        </div>
      {{/if}}
      {{#if element.images.[5]}}
        <div class="carousel-item">
          <img class="d-block carousel-img" src="{{element.images.[5]}}" alt="Third slide">
        </div>
      {{/if}}
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev" id="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next" id="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
